{
  "rewrites": [
    { "source": "/whitepaper.pdf", "destination": "/public/whitepaper.pdf" }
  ],
  "headers": [
    {
      "source": "/public/whitepaper.pdf",
      "headers": [
        { "key": "Content-Type", "value": "application/pdf" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
